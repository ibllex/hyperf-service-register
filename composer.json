{
    "name": "ibllex/hyperf-service-register",
    "description": "Service registration component for hyperf",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "hyperf/consul": "~2.0.0",
        "hyperf/http-server": "~2.0.0",
        "hyperf/contract": "~2.0.0"
    },
    "require-dev": {
        "google/protobuf": "^3.6.1",
        "hyperf/event": "~2.0.0",
        "hyperf/framework": "^2.0",
        "phpunit/phpunit": "^7.0.0"
    },
    "autoload": {
        "psr-4": {
            "iBllex\\ServiceRegister\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iBllexTest\\ServiceRegister\\": "tests/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "iBllex\\ServiceRegister\\ConfigProvider"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always tests"
    }
}